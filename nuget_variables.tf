variable nuget_local_repo_key {
  description = "contains the nuget local repository key"
  default     = "nuget-local-test-repo"
}

variable nuget_local_package_type {
  description = "contains the nuget local repository package type"
  default     = "nuget"
}

variable nuget_local_repo_description {
  description = "contains the nuget local repository description"
  default     = "this repository contains the nuget packages"
}
