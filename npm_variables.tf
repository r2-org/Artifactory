variable npm_local_repo_key {
  description = "contains the npm local repository key"
  default     = "npm-local-test-repo"
}

variable npm_local_package_type {
  description = "contains the npm local repository package type"
  default     = "npm"
}

variable npm_local_repo_description {
  description = "contains the npm local repository description"
  default     = "this repository contains the npm packages"
}
